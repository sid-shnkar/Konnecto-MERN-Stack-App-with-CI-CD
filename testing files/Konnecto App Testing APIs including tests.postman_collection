			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Status code is 201\", function () {\r",
							"    pm.response.to.have.status(201);\r",
							"});\r",
							"\r",
							"const response = pm.response.json();\r",
							"\r",
							"pm.test(\"Response is an object\", function (){\r",
							"   pm.expect(response).to.be.an('object');\r",
							"});\r",
							"\r",
							"pm.test(\"Comment should be created on matched postId\", function () {\r",
							"    \r",
							"    // console.log(pm.request.url.path);\r",
							"     pm.expect(pm.request.url.path.length).to.be.eql(3);\r",
							"\r",
							"     const postId = pm.request.url.path[1];\r",
							"     \r",
							"     pm.expect(JSON.parse(pm.request.body.raw)).to.haveOwnProperty(\"text\");\r",
							"\r",
							"     const commentText = JSON.parse(pm.request.body.raw).text;\r",
							"     //console.log(pm.request.body);\r",
							"    // //console.log(requestedId);\r",
							"\r",
							"    const currentUserId = pm.collectionVariables.get(\"currentUserId\");\r",
							"\r",
							"    pm.expect(response).to.haveOwnProperty(\"postId\");\r",
							"    pm.expect(response).to.haveOwnProperty(\"userId\");\r",
							"    pm.expect(response).to.haveOwnProperty(\"text\");\r",
							"\r",
							"    pm.expect(response.postId).to.be.equal(postId);\r",
							"    pm.expect(response.userId).to.be.equal(currentUserId);\r",
							"    pm.expect(response.text).to.be.equal(commentText);\r",
							"\r",
							"});\r",
